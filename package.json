{
  "name": "kb-biblioteksstatistik-mcp",
  "version": "2.0.0",
  "description": "MCP server för Kungliga Bibliotekets öppna biblioteksstatistik API",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "kb-biblioteksstatistik-mcp": "build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:local": "tsc --build tsconfig.local.json",
    "watch": "tsc --watch",
    "start": "node build/index.js",
    "start:server": "node build/server.js",
    "install:local": "npm install --omit=optional && npm run build:local",
    "install:remote": "npm install && npm run build",
    "postinstall": "npm run build:local || npm run build"
  },
  "keywords": [
    "mcp",
    "kungliga-biblioteket",
    "biblioteksstatistik",
    "library-statistics",
    "opendata",
    "mcp-server",
    "remote-server",
    "sse"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KSAklfszf921/KB-Biblioteksstatistik-MCP.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4"
  },
  "optionalDependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.7.2"
  },
  "files": [
    "build/",
    "terms",
    "README.md",
    "LICENSE"
  ]
}
